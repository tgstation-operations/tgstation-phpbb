    "homepage": "https://caforum/fr/forum",
    "version": "1.0.0",
    "time": "2020-12-06",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Origin",
            "email": "contact@caforum.fr",
            "homepage": "https://caforum.fr/forum/",
