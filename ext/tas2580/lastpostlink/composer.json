{
    "name": "tas2580/lastpostlink",
    "type": "phpbb-extension",
    "description": "Changes links to last post so that are all links are equal",
    "homepage": "https://tas2580.net/downloads/phpbb-lastpostlink/",
    "version": "1.0.0",
    "keywords": [
        "phpbb",
        "extension",
        "seo",
        "url"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "tas2580",
            "email": "mail@tas2580.net",
            "homepage": "https://tas2580.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0",
        "phpbb/phpbb": ">=3.1.4-RC1,<3.2.0@dev"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "tas2580 lastpostlink",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.4-RC1,<3.2.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/lastpostlink",
            "filename": "version_check"
        }
    }
}